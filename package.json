{
	"name": "filebox",
	"version": "1.0.0",
	"description": "file storage app",
	"main": "index.js",
	"scripts": {
		"start": "SET NODE_ENV=development & parcel src/index.html --open",
		"build": "SET NODE_ENV=production & parcel build src/index.html --public-url ./",
		"test": "jest",
		"lint": "eslint src/ --ext .js --ext .jsx"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|less|scss|sass)$": "identity-obj-proxy"
		},
		"collectCoverageFrom": [
			"src/**"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sibasishm/filebox.git"
	},
	"keywords": [
		"files",
		"storage"
	],
	"author": "Sibasish Mohanty",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/sibasishm/filebox/issues"
	},
	"homepage": "https://github.com/sibasishm/filebox#readme",
	"dependencies": {
		"@emotion/react": "^11.1.4",
		"@emotion/styled": "^11.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"tailwindcss": "^2.0.2",
		"twin.macro": "^2.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@babel/preset-react": "^7.12.13",
		"@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.5",
		"@testing-library/user-event": "^12.6.3",
		"babel-eslint": "^10.1.0",
		"babel-plugin-macros": "^3.0.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"eslint": "^7.19.0",
		"eslint-plugin-react": "^7.22.0",
		"jest": "^26.6.3",
		"parcel-bundler": "^1.12.4",
		"prettier": "^2.2.1"
	}
}
